{
  "name": "applywallet",
  "displayName": "applywallet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently  \"yarn dev:app\" \"yarn dev:ext\"",
    "dev:app": "next dev",
    "dev:ext": "plasmo dev",
    "dev:app:cdt": "yarn cdt && yarn dev",
    "build": "concurrently  \"yarn build:app\" \"yarn build:ext\"",
    "build:app": "next build",
    "build:ext": "plasmo build",
    "build:app:cdt": "yarn cdt && yarn build",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "format": "prettier --write 'app/**/*.{ts,tsx}'",
    "format:css": "stylelint --fix '**/*.scss'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "update-next": "yarn add next@latest react@latest react-dom@latest eslint-config-next@latest @next/bundle-analyzer@latest @types/react @types/react-dom",
    "cdt": "tsx ./scripts/createDirectoryTree.ts"
  },
  "dependencies": {
    "@dx-kit/react-conditional-match": "^3.0.0",
    "@formatjs/intl-localematcher": "^0.2.32",
    "@hookform/resolvers": "^2.9.11",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@plasmohq/storage": "^1.8.1",
    "@prisma/client": "^5.5.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@sentry/nextjs": "^7.77.0",
    "@types/dagre": "^0.7.51",
    "@types/gtag.js": "^0.0.17",
    "@vitest/ui": "^0.34.6",
    "axios": "^1.5.1",
    "clsx": "^1.2.1",
    "concurrently": "^8.2.2",
    "contentlayer": "^0.3.4",
    "cva": "npm:class-variance-authority@^0.7.0",
    "dagre": "^0.8.5",
    "dayjs": "^1.11.10",
    "framer-motion": "^10.16.4",
    "google-spreadsheet": "^4.1.0",
    "loglevel": "^1.8.1",
    "lucide-react": "^0.268.0",
    "negotiator": "^0.6.3",
    "next": "^14.0.1",
    "next-auth": "^4.24.3",
    "next-contentlayer": "^0.3.4",
    "next-international": "^1.1.3",
    "nextjs-toploader": "^1.5.3",
    "plasmo": "^0.83.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-responsive-masonry": "^2.1.7",
    "react-toastify": "^9.1.3",
    "react-use": "^17.4.0",
    "reactflow": "^11.9.4",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-pretty-code": "^0.10.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "server-only": "^0.0.1",
    "shiki": "^0.14.5",
    "swr": "^2.2.4",
    "tailwind-merge": "^1.14.0",
    "theme-change": "^2.5.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@next/bundle-analyzer": "^14.0.1",
    "@svgr/webpack": "^7.0.0",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/chrome": "0.0.245",
    "@types/google-spreadsheet": "^3.3.2",
    "@types/negotiator": "^0.6.2",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@types/react-responsive-masonry": "^2.1.2",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.16",
    "daisyui": "^3.9.3",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.1",
    "eslint-config-prettier": "^8.10.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.3.0",
    "postcss": "^8.4.31",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "prisma": "^5.5.2",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-clean-order": "^5.2.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-order": "^6.0.3",
    "tailwindcss": "^3.3.5",
    "tsx": "^3.14.0",
    "typescript": "5.1.6",
    "vitest": "^0.34.6",
    "yargs": "^17.7.2"
  },
  "manifest": {
    "name": "applywallet",
    "default_locale": "en",
    "host_permissions": [
      "http://localhost:3000/api/*"
    ],
    "permissions": [
      "tabs",
      "storage",
      "contextMenus"
    ]
  }
}
