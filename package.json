      "@types/react-dom": "19.0.3"
    }
  },
  "manifest": {
    "name": "applywallet",
    "default_locale": "en",
    "host_permissions": [
      "http://localhost:3000/api/*"
    ],
    "permissions": [
      "tabs",
      "storage",
      "contextMenus"
    ]
  }
