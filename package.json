{
  "name": "applywallet",
  "displayName": "applywallet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently  \"yarn dev:app\" \"yarn dev:ext\"",
    "dev:app": "next dev --turbopack",
    "dev:app:webpack": "next dev",
    "dev:ext": "plasmo dev",
    "dev:app:cdt": "yarn cdt && yarn dev",
    "build": "concurrently  \"yarn build:app\" \"yarn build:ext\"",
    "build:app": "next build",
    "build:app:prod": "yarn cdt && yarn build:app",
    "build:ext": "plasmo build",
    "build:content": "contentlayer2 build",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "format": "prettier --write 'app/**/*.{ts,tsx}'",
    "format:css": "stylelint --fix '**/*.scss'",
    "test": "TZ=UTC vitest",
    "test:ui": "TZ=UTC vitest --ui",
    "coverage": "TZ=UTC vitest run --coverage",
    "update-next": "yarn add next@latest react@latest react-dom@latest eslint-config-next@latest @next/bundle-analyzer@latest @types/react @types/react-dom",
    "cdt": "tsx ./scripts/createFolderStructure.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "knip": "knip --reporter markdown > knip-output.md",
    "knip:fix": "knip --production --fix"
  },
