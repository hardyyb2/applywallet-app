{
  "name": "applywallet",
  "displayName": "applywallet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently  \"pnpm dev:app\" \"pnpm dev:ext\"",
    "dev:app": "next dev --turbopack",
    "dev:ext": "plasmo dev",
    "dev:app:cdt": "pnpm cdt && pnpm dev",
    "build": "concurrently  \"pnpm build:app\" \"pnpm build:ext\"",
    "build:app": "next build",
    "build:ext": "plasmo build",
    "build:cdt": "pnpm cdt && pnpm build",
    "build:content": "contentlayer2 build",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "format": "prettier --write 'app/**/*.{ts,tsx}'",
    "format:css": "stylelint --fix '**/*.scss'",
    "test": "TZ=UTC vitest",
    "test:ui": "TZ=UTC vitest --ui",
    "coverage": "TZ=UTC vitest run --coverage",
    "update-next": "pnpm add next@latest react@latest react-dom@latest eslint-config-next@latest @next/bundle-analyzer@latest @types/react @types/react-dom",
    "cdt": "tsx ./scripts/createFolderStructure.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
